{
  "env": {
    "mocha": true
  },
  "rules": {
    "max-len": 0,
    "no-shadow": 0,
    "func-names": 0,
    "padded-blocks": 0,
    "prefer-arrow-callback": 0,
    "no-unused-expressions": 0,
    "import/no-extraneous-dependencies": [2, {"devDependencies": true}]
  },
  "globals": {
    "chai": true,
    "expect": true,
    "hooks": true
  }
}
